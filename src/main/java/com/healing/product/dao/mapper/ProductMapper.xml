<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.productMapper">   <!-- namespace : mapper를 식별해주는 식별자 -->
	<select id="productFlightRead" parameterType="int" resultType="com.healing.product.dto.FlightDto"> 
		select * from flight where flight_number=#{flight_number}	
	</select>
</mapper>
	